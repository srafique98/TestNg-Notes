<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Auth Suite" verbose="1" parallel="tests" thread-count="10">

    <listeners>
        <listener class-name="com.solvd.laba.TestNGListener"/>
    </listeners>

    <test name="Login Test" parallel="methods" thread-count="5">
        <classes>
            <class name="com.solvd.laba.LogInTest"/>
        </classes>
    </test>

    <test name="Logout Test 2">
        <classes>
            <class name="com.solvd.laba.LogInTest"/>
        </classes>
    </test>

    <!--    <test name="Forgot password Test">-->

